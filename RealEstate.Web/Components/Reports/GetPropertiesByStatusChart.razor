@inherits GetPropertiesByStatusChartComponent

@if (Data.Count > 0)
{
    <MudChart ChartType="ChartType.Pie" InputData="@Data.ToArray()" InputLabels="@Labels.ToArray()" Width="100%" Height="200px"/>
}
else
{
    <div class="d-flex align-center justify-center">
        <MudProgressCircular Indeterminate="true" Color="Color.Primary" />
    </div>
}